{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "环境变量管理工具",
  "version": "1.0.12",
  "identifier": "com.administrator.hold-config",
  "build": {
    "beforeDevCommand": "pnpm dev",
    "devUrl": "http://localhost:8777",
    "beforeBuildCommand": "pnpm build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "环境变量管理工具",
        "width": 900,
        "height": 700
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": [
      "nsis"
    ],
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}